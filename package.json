{
  "name": "front-end-fisheye",
  "version": "1.0.0",
  "main": "index.js",
  "types": "index.d.ts",
  "private": true,
  "scripts": {
    "sass": "sass -w ./style.scss:./css/style.css",
    "lint": "eslint .",
    "prettier": "prettier --ignore-path .gitignore **/*.{js,json,html,scss}",
    "format": "yarn prettier --write",
    "check-format": "yarn prettier --list-different",
    "validate": "npm-run-all --parallel check-format lint",
    "postinstall": "husky install"
  },
  "repository": "https://github.com/MichelD-dev/Front-End-Fisheye.git",
  "author": "michelD-dev <delaunaymichel@hotmail.fr>",
  "license": "MIT",
  "packageManager": "yarn@3.2.0",
  "devDependencies": {
    "eslint": "^8.22.0",
    "eslint-config-prettier": "^8.5.0",
    "husky": "^8.0.1",
    "lint-staged": "^13.0.3",
    "prettier": "^2.7.1",
    "yarn-run-all": "^3.1.1"
  }
}
